{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "globals": {
    "exampleGlobalVariable": true
  },
  "extends": "eslint:recommended",
    "rules": {
      "eqeqeq": 1,
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
  "plugins": [
    "import",
    "node",
    "promise",
    "standard"
  ]
}